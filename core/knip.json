{
  "$schema": "https://unpkg.com/knip@latest/schema.json",
  "include": ["files", "duplicates"],
  "entry": [
    "src/index.ts",
    "src/webhook-server.ts",
    "src/.opencode/tool/*.ts"
  ],
  "project": ["src/**/*.ts"],
  "ignore": [
    "src/.opencode-runtime/**/*",
    "dist/**/*",
    "eslint.config.js"
  ],
  "ignoreExportsUsedInFile": true,
  "ignoreDependencies": [
    "tsc-alias",
    "tsconfig-paths"
  ]
}
